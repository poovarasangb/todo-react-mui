{
  "name": "book-my-show-clone",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://github.com/poovarasangb/todo-react-mui.git",
  "scripts": {
    "gitClean": "git clean -x -d -f -e .history -e src -e build",
    "clean": "rm -rf node_modules package-lock.json output pnpm-lock.yaml",
    "ins": "pnpm i --registry https://registry.npmjs.com",
    "postinstall": "./node_modules/.bin/husky install",
    "cleanIns": "npm run clean && pnpm run ins",
    "devServer": "webpack serve --config  ./build/configs/webpack.common.js",
    "devDBServer": "json-server -p 2020 --watch db.json --id todoId",
    "cleanWatch": "npm run cleanIns && npm run watch",
    "hook-script-pre-commit": "node --max-old-space-size=8192 ./build/gitScripts/preCommit.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "~11.10.6",
    "@emotion/styled": "~11.10.6",
    "@mui/icons-material": "~5.11.11",
    "@mui/material": "~5.11.13",
    "@mui/system": "~5.11.13",
    "@mui/x-data-grid": "~6.0.2",
    "@mui/x-date-pickers": "^6.0.2",
    "axios": "~1.3.4",
    "clsx": "~1.2.1",
    "css-loader": "~6.7.3",
    "date-fns": "^2.29.3",
    "dayjs": "^1.11.7",
    "eslint-plugin-react-hooks": "~4.6.0",
    "json-server": "~0.17.2",
    "node-sass": "~7.0.3",
    "postcss": "~8.4.21",
    "postcss-loader": "~7.0.2",
    "prop-types": "~15.8.1",
    "react": "~18.2.0",
    "react-custom-events": "~1.1.1",
    "react-dom": "~18.2.0",
    "react-hooks": "~1.0.1",
    "react-transition-group": "~4.4.5",
    "sass": "~1.56.1",
    "uuid": "~9.0.0"
  },
  "devDependencies": {
    "@popperjs/core": "~2.11.6",
    "esbuild-loader": "~2.20.0",
    "eslint": "~8.28.0",
    "eslint-plugin-import": "~2.27.5",
    "eslint-plugin-n": "~15.5.1",
    "eslint-plugin-promise": "~6.1.1",
    "eslint-plugin-react": "~7.31.11",
    "eslint-webpack-plugin": "~3.2.0",
    "html-webpack-plugin": "~5.5.0",
    "husky": "^8.0.2",
    "mini-css-extract-plugin": "~2.7.0",
    "sass-loader": "~13.2.0",
    "terminal-kit": "^3.0.0",
    "webpack": "~5.75.0",
    "webpack-cli": "~5.0.0",
    "webpack-dev-server": "~4.11.1"
  },
  "husky": {
    "hooks": {
        "pre-commit": "yarn pretty-quick --staged"
    }
  }
}
